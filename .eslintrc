    "computed-property-spacing": ["off"],
    "consistent-return": ["error"],
    "consistent-this": ["off"],
    "constructor-super": ["error"],
    "curly": ["off"],
    "default-case": ["off"],
    "default-case-last": ["error"],
    "default-param-last": ["off"],
    "dot-location": ["off"],
    "dot-notation": ["off"],
    "eol-last": ["off"],
    "eqeqeq": [
