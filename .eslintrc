{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "globals": {
    "Promise": "readonly",
    "describe": "readonly",
    "expect": "readonly",
    "it": "readonly"
  },
  "plugins": ["@typescript-eslint", "eslint-plugin-tsdoc", "import"],
